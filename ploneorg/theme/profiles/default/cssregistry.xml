<?xml version="1.0"?>
<!-- This file holds the setup configuration for the portal_css tool. -->

<object name="portal_css">

 <!-- Stylesheets that will be registered with the portal_css tool are defined
      here. You can also specify values for existing resources if you need to
      modify some of their properties.
      Stylesheet elements accept these parameters:
      - 'id' (required): it must respect the name of the css or DTML file
        (case sensitive). '.dtml' suffixes must be ignored.
      - 'expression' (optional - default: ''): a tal condition.
      - 'media' (optional - default: ''): possible values: 'screen', 'print',
        'projection', 'handheld'...
      - 'rel' (optional - default: 'stylesheet')
      - 'title' (optional - default: '')
      - 'rendering' (optional - default: 'import'): 'import', 'link' or
        'inline'.
      - 'enabled' (optional - default: True): boolean
      - 'cookable' (optional - default: True): boolean (aka 'merging allowed')
      - 'insert_before', 'insert_after' (optional - default: '') resource id
      See registerStylesheet() arguments in
      ResourceRegistries/tools/CSSRegistry.py for the latest list of all
      available keys and default values.
      -->

   <stylesheet title=""
    id="++theme++ploneorg/static/main.css"
    media="screen" rel="stylesheet" rendering="import"
    cacheable="True" compression="safe" cookable="True"
    enabled="1" expression="" />

  <!--  Sunburst Reset -->
    <stylesheet title=""
      id="++framework++plone/sunburst/css/reset.css"
      media="screen" rel="stylesheet" rendering="link"
      cacheable="True" compression="safe" cookable="True"
      enabled="1" expression="" />
  <!--  Do not use normalize.css with sunburst only css files -->
    <stylesheet title=""
      id="++theme++ploneorg/css/normalize.css"
      media="screen" rel="stylesheet" rendering="link"
      cacheable="True" compression="safe" cookable="True"
      enabled="0" expression="" />   
  <!-- Basic Styling --> 
    <stylesheet title=""
      id="++framework++plone/sunburst/css/base.css"
      media="screen" rel="stylesheet" rendering="link"
      cacheable="True" compression="safe" cookable="True"
      enabled="1" expression="" />
    <stylesheet title=""
      id="++framework++plone/sunburst/css/columns.css"
      media="screen" rel="stylesheet" rendering="link"
      cacheable="True" compression="safe" cookable="True"
      enabled="1" expression="" />
    
  <!-- Public Styling -->
    <stylesheet title=""
      id="++framework++plone/sunburst/css/public.css"
      media="screen" rel="stylesheet" rendering="link"
      cacheable="True" compression="safe" cookable="True"
      enabled="1" expression="" />
    <stylesheet title=""
      id="++framework++plone/sunburst/css/portlets.css"
      media="screen" rel="stylesheet" rendering="link"
      cacheable="True" compression="safe" cookable="True"
      enabled="1" expression="" />
    <stylesheet title=""
      id="++framework++plone/sunburst/css/deprecated.css"
      media="screen" rel="stylesheet" rendering="link"
      cacheable="True" compression="safe" cookable="True"
      enabled="1" expression="" />
    <stylesheet title=""
      id="++framework++plone/sunburst/css/navtree.css"
      media="screen" rel="stylesheet" rendering="link"
      cacheable="True" compression="safe" cookable="True"
      enabled="1" expression="" />
    <stylesheet title=""
      id="++framework++plone/sunburst/css/invisibles.css"
      media="screen" rel="stylesheet" rendering="link"
      cacheable="True" compression="safe" cookable="True"
      enabled="1" expression="" />
    <stylesheet title=""
      id="++framework++plone/sunburst/css/forms.css"
      media="screen" rel="stylesheet" rendering="link"
      cacheable="True" compression="safe" cookable="True"
      enabled="1" expression="" />
  <!-- Authenticated Styling -->
    <stylesheet title=""
      id="++framework++plone/sunburst/css/member.css"
      media="screen" rel="stylesheet" rendering="link"
      cacheable="True" compression="safe" cookable="True"
      enabled="1" expression="" authenticated="True"/>
    <stylesheet title=""
      id="++framework++plone/sunburst/css/authoring.css"
      media="screen" rel="stylesheet" rendering="link"
      cacheable="True" compression="safe" cookable="True"
      enabled="1" expression="" authenticated="True"/>
    <stylesheet title=""
      id="++framework++plone/sunburst/css/controlpanel.css"
      media="screen" rel="stylesheet" rendering="link"
      cacheable="True" compression="safe" cookable="True"
      enabled="1" expression="" authenticated="True"/>
      
  <!-- Media/Mode Specific Styling -->
    <stylesheet title=""
      id="++framework++plone/css/respond.css"
      media="screen" rel="stylesheet" rendering="link"
      cacheable="True" compression="safe" cookable="True"
      enabled="1" expression="" />
    <stylesheet title=""
      id="++framework++plone/sunburst/css/print.css"
      media="print" rel="stylesheet" rendering="link"
      cacheable="True" compression="safe" cookable="True"
      enabled="1" expression="" />
    <stylesheet title=""
      id="++framework++plone/sunburst/css/mobile.css"
      media="handheld, screen and (max-device-width: 480px)" rel="stylesheet" rendering="link"
      cacheable="True" compression="safe" cookable="True"
      enabled="0" expression="" />
    <stylesheet title=""
      id="++framework++plone/sunburst/css/RTL.css"
      media="screen" rel="stylesheet" rendering="link"
      cacheable="True" compression="safe" cookable="True"
      enabled="1" expression="python:portal.restrictedTraverse('@@plone_portal_state').is_rtl()" />
    <stylesheet title=""
      id="++framework++plone/sunburst/css/IEFixes.css"
      media="screen" rel="stylesheet" rendering="link"
      cacheable="True" compression="safe" cookable="True"
      enabled="1" expression="" conditionalcomment="lt IE 8" />
    
 
</object>
